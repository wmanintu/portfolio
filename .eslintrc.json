{
  "env": {
    "browser": true,
    "es2022": true
  },
  "extends": ["standard", "plugin:vue/vue3-essential"],
  "overrides": [
    {
      "env": {
        "node": true
      },
      "files": [".eslintrc.{js,cjs}"],
      "parserOptions": {
        "sourceType": "script"
      }
    }
  ],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module",
    "parser": "babel-eslint"
  },
  "plugins": ["vue"],
  "rules": {}
}
